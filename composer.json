{
    "name": "lireincore/imgcache-bundle",
    "description": "The lireincore/imgcache integration for the Symfony framework",
    "type": "symfony-bundle",
    "keywords": ["symfony", "bundle", "image", "cache", "preset", "thumb", "effect", "resize"],
    "homepage": "https://github.com/lireincore/LireinCoreImgCacheBundle",
    "license": "MIT",
    "support": {
        "issues": "https://github.com/lireincore/LireinCoreImgCacheBundle/issues",
        "source": "https://github.com/lireincore/LireinCoreImgCacheBundle"
    },
    "authors": [
        {
            "name": "lirein",
            "email": "lireincore@gmail.com"
        }
    ],
    "require": {
        "lireincore/imgcache": "^0.4",
        "symfony/config": "^3.4 || ^4.0",
        "symfony/http-kernel": "^3.4 || ^4.0",
        "symfony/dependency-injection": "^3.4 || ^4.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^7",
        "roave/security-advisories": "dev-master"
    },
    "autoload": {
        "psr-4": {
            "LireinCore\\ImgCacheBundle\\": ""
        },
        "exclude-from-classmap": [ "/Tests/" ]
    },
    "autoload-dev": {
        "psr-4": {
            "LireinCore\\ImgCacheBundle\\Tests\\": "/Tests/"
        }
    }
}
